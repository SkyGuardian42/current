{
  "name": "current",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "node --require dotenv/config --experimental-specifier-resolution=node ./src/bin/www",
    "test": "node --experimental-vm-modules node_modules/.bin/jest --forceExit",
    "local-test": "npm run local-pretest && DOTENV_CONFIG_PATH=.env.test node --experimental-vm-modules --require dotenv/config node_modules/.bin/jest --forceExit ; npm run local-cleanup",
    "local-pretest": "node ./test/startTest.js",
    "local-cleanup": "node ./test/endTest.js"
  },
  "dependencies": {
    "@sendgrid/mail": "^7.4.2",
    "bcrypt": "^5.0.1",
    "cookie-parser": "~1.4.4",
    "cors": "^2.8.5",
    "debug": "~2.6.9",
    "dotenv": "^8.2.0",
    "express": "~4.16.1",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^5.12.3",
    "mongoose-unique-validator": "^2.0.3",
    "morgan": "~1.9.1",
    "multer": "^1.4.2",
    "sharp": "^0.28.1",
    "validator": "^13.5.2"
  },
  "devDependencies": {
    "jest": "^26.5.3",
    "supertest": "^6.1.3"
  },
  "type": "module",
  "jest": {
    "testEnvironment": "node",
    "coveragePathIgnorePatterns": [
      "/node_modules/"
    ]
  }
}
